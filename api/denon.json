{
  "scripts": {
    "start": {
      "cmd": "deno run --allow-net --allow-env src/main.ts"
    }
  },
  "watcher": {
    "exts": ["js", "jsx", "ts", "tsx", "json"],
    "match": ["**/*.*"],
    "skip": ["Dockerfile"]
  },
  "logger": {
    "fullscreen": false,
    "quiet": false,
    "debug": true
  }
}